{
  "name": "data-editor",
  "version": "1.0.0",
  "description": "an editor for tabular data built with virtual-dom and base-element",
  "main": "index.js",
  "style": "style.css",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "tape tests/*.js | tap-spec",
    "test-browser": "budo tests/shareabouts.js -- -t [ envify --NODE_ENV development ]",
    "example-shareabouts": "budo examples/shareabouts/index.js --live --dir examples/shareabouts -- -t [ envify --NODE_ENV development ]",
    "example-basic-css": "sheetify examples/basic/style.css > examples/basic/bundle.css",
    "example-basic": "budo examples/basic/index.js --live --onupdate 'npm run example-basic-css' --dir examples/basic -- -t [ envify --NODE_ENV development ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/editdata/data-editor.git"
  },
  "keywords": [
    "data",
    "editor"
  ],
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/editdata/data-editor/issues"
  },
  "homepage": "https://github.com/editdata/data-editor#readme",
  "dependencies": {
    "base-element": "^3.0.1",
    "csv-write-stream": "^1.0.0",
    "cuid": "^1.3.8",
    "data-format": "editdata/data-format",
    "data-set": "^4.0.0",
    "dom-value": "0.0.0",
    "extend": "^3.0.0",
    "from2": "^2.1.0",
    "from2-array": "0.0.4",
    "inherits": "^2.0.1",
    "isarray": "0.0.1",
    "leaflet": "^0.7.5",
    "mapbox.js": "^2.2.2",
    "through2": "^2.0.0",
    "view-list": "^2.0.0"
  },
  "devDependencies": {
    "budo": "^5.1.1",
    "corsify": "^2.1.0",
    "data-grid": "editdata/data-grid",
    "envify": "^3.4.0",
    "http-proxy": "^1.11.2",
    "level-js": "^2.2.2",
    "levelup": "^1.2.1",
    "request": "^2.64.0",
    "sheetify": "^2.0.3",
    "virtual-dom": "^2.1.1",
    "xhr": "^2.1.0"
  }
}
